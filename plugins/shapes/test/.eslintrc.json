{
    "extends": "../../../.eslintrc.json",
    "env": {
        "commonjs": false,
        "mocha": true
    },
    "globals": {
        "chai": false,
        "expect": false,
        "sinon": false,
        "Fae": false
    },
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "strict": 0,
        "no-unused-expressions": 0,
        "func-names": 0,
        "no-var": 0
    }
}
